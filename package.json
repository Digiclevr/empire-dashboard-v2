{
  "name": "blueocean-dashboard",
  "version": "2.0.0",
  "description": "BlueOcean Dashboard - Supervision Nexia de l'écosystème BlueOcean",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.3.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "empire",
    "nexia",
    "directus",
    "dashboard",
    "blueocean",
    "react"
  ],
  "author": "Ludovic Pilet",
  "license": "MIT"
}
